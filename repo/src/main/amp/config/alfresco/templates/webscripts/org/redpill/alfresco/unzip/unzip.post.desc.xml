<webscript>
  <shortname>Unzip WebScript</shortname>
  <description><![CDATA[
  Takes a source and a target nodeRef and processes the request.
  ]]>
  </description>
  <url>/org/redpill/unzip?source={source}&amp;target={target}&amp;async={async}</url>
  <format default="json">argument</format>
  <authentication>user</authentication>
  <transaction>required</transaction>
  <family>Redpill-Linpro</family>
</webscript>